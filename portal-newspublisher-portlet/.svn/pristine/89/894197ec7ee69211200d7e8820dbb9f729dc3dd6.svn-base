<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>1</portlet-name>
		<icon>/icon.png</icon>
		<configuration-action-class>com.dtt.portal.newspublisher.action.ConfigurationActionImpl</configuration-action-class>
		<instanceable>true</instanceable>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<header-portlet-css>/css/config.css</header-portlet-css>
		<header-portlet-css>/css/slider_horizontal.css</header-portlet-css>
		<header-portlet-javascript>/js/main.js</header-portlet-javascript>
		<header-portlet-javascript>/js/jquery.marquee.min.js</header-portlet-javascript>
		<header-portlet-javascript>/js/jssor.core.min.js</header-portlet-javascript>
		<header-portlet-javascript>/js/jssor.slider.mini.js</header-portlet-javascript>
		<!-- <header-portlet-javascript>/js/action.js</header-portlet-javascript> -->
		<css-class-wrapper>portal-newspublisher-portlet</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>